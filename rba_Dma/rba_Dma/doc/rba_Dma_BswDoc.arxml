<E TYPE="BOLD">rba_Dma_SetUpCh</E> (rba_Dma_Id_ten              const idChan_cen,
                                rba_Dma_SetUpCh_tst const * const adrConfig_cpcst,
                                uint32                    * const stErr_cpu32)</L-5>
                                                                                        </VERBATIM>
                                                                                    </ENTRY>
                                                                                </ROW>
                                                                                <ROW VALIGN="TOP">
                                                                                    <ENTRY COLNAME="col1" ROTATE="0" VALIGN="TOP">
