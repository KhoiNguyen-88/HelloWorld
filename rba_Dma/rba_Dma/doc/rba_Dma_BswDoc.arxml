#define RBA_DMA_SETUPCH_CHNOTREADY_ERR_POS  16ul
#define RBA_DMA_SETUPCH_SRCBUFSIZE_ERR_POS  17ul
#define RBA_DMA_SETUPCH_DESTBUFSIZE_ERR_POS 18ul</L-5>
