                                                                <LONG-NAME>
                                                                    <L-4 L="EN">Data types</L-4>
                                                                </LONG-NAME>
                                                                <CHAPTER>
                                                                    <SHORT-NAME>ID75945865043811</SHORT-NAME>
                                                                    <LONG-NAME>
                                                                        <L-4 L="EN">rba_Dma_Id_ten</L-4>
                                                                    </LONG-NAME>
                                                                    <TABLE>
                                                                        <TABLE-CAPTION>
                                                                            <SHORT-NAME>ID04526344146667</SHORT-NAME>
                                                                            <LONG-NAME>
                                                                                <L-4 L="EN">rba_Dma_Id_ten</L-4>
                                                                            </LONG-NAME>
                                                                        </TABLE-CAPTION>
                                                                        <TGROUP ALIGN="LEFT" COLS="2">
                                                                            <COLSPEC COLNAME="col1" COLNUM="1"/>
                                                                            <COLSPEC COLNAME="col2" COLNUM="2"/>
                                                                            <TBODY VALIGN="TOP">
                                                                                <ROW VALIGN="TOP">
                                                                                    <ENTRY COLNAME="col1" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">Name:</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                    <ENTRY COLNAME="col2" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">rba_Dma_Id_ten</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                </ROW>
                                                                                <ROW VALIGN="TOP">
                                                                                    <ENTRY COLNAME="col1" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">Type:</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                    <ENTRY COLNAME="col2" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">Enumeration</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                </ROW>
                                                                                <ROW VALIGN="TOP">
                                                                                    <ENTRY COLNAME="col1" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">Range:</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                    <ENTRY COLNAME="col2" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">List of all configured DMA channels across all post build variants.
                                              Contents are configuration dependent. Only two entries are listed as example:</L-1>
                                                                                        </P>
                                                                                        <LIST TYPE="UNNUMBER">
                                                                                            <ITEM>
                                                                                                <P>
                                                                                                    <L-1 L="EN">rba_Dma_AdcDs_Value_0: derived from short name 'AdcDs_Value_0'</L-1>
                                                                                                </P>
                                                                                            </ITEM>
                                                                                            <ITEM>
                                                                                                <P>
                                                                                                    <L-1 L="EN">rba_Dma_rba_Msc_HwUnit1_Us: derived from short name 'rba_Msc_HwUnit1_Us'</L-1>
                                                                                                </P>
                                                                                            </ITEM>
                                                                                            <ITEM>
                                                                                                <P>
                                                                                                    <L-1 L="EN">RBA_DMA_NUM_PB_VRNT_E: Last element.</L-1>
                                                                                                </P>
                                                                                            </ITEM>
                                                                                        </LIST>
                                                                                    </ENTRY>
                                                                                </ROW>
                                                                                <ROW VALIGN="TOP">
                                                                                    <ENTRY COLNAME="col1" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">Description:</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                    <ENTRY COLNAME="col2" ROTATE="0" VALIGN="TOP">
                                                                                        <P>
                                                                                            <L-1 L="EN">This enumeration is a list of all configured DMA channels, it
                                              contains each short name of the entire DMA configuration. Last enum in the
                                              list is always RBA_DMA_NUM_IDS_E. See example for details. rba_Dma_Id_ten
                                              is used as parameter for the interface rba_Dma_GetIdx().</L-1>
                                                                                        </P>
                                                                                    </ENTRY>
                                                                                </ROW>
                                                                            </TBODY>
                                                                        </TGROUP>
                                                                    </TABLE>
                                                                    <P>
                                                                        <L-1 L="EN"/>
                                                                    </P>
                                                                    <P>
                                                                        <L-1 L="EN"/>
                                                                    </P>
                                                                    <P>
                                                                        <L-1 L="EN">The resulting generated enumeration looks like:</L-1>
                                                                    </P>
                                                                    <P>
                                                                        <L-1 L="EN"/>
                                                                    </P>
                                                                    <VERBATIM ALLOW-BREAK="1">
                                                                        <L-5 L="EN" xml:space="default">typedef enum
{
    rba_Dma_AdcDs_Gtm_2,
    rba_Dma_AdcDs_Value_0,
    rba_Dma_AdcDs_Value_2,
    rba_Dma_rba_AdcIfx_Mux0,
    rba_Dma_rba_AdcIfx_Mux1,
    rba_Dma_rba_AdcIfx_Mux2,
    rba_Dma_rba_AdcIfx_Mux3,
    rba_Dma_rba_Msc_HwUnit0_Us,
    rba_Dma_rba_Msc_HwUnit1_Us,
    rba_Dma_rba_SpiIfx_HwUnit0_Rx,
    rba_Dma_rba_SpiIfx_HwUnit0_Tx1,
    rba_Dma_rba_SpiIfx_HwUnit0_Tx2,
    RBA_DMA_NUM_IDS_E
} rba_Dma_Id_ten;</L-5>
                                                                    </VERBATIM>
                                                                </CHAPTER>
